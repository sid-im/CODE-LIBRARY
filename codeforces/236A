#include<bits/stdc++.h>
#define int long long int
using namespace std;

void solve(){
string s;
cin>>s;
map<char,int> mp;
for(int i=0;i<s.length();i++){
    mp[s[i]]++;
}
int c=mp.size();

if(c%2==0)cout<<"CHAT WITH HER!"<<endl;
else cout<<"IGNORE HIM!"<<endl;
}




int32_t main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
int t=1;
//cin>>t;
while(t--)

{


    solve();
}


    return 0;
}
