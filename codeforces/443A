#include<bits/stdc++.h>
using namespace std;

int main()
{
    string str;
    map<char,int> mp;

    getline(cin, str);
  for(int i=0;str[i]!='}';i++){
        if(str[i]!='{' && str[i]!=',' && str[i]!='}' && str[i]!=' '){
       mp[str[i]]++;
       //cout<<str[i]<<endl;
  }
  }

cout<<mp.size()<<endl;
    return 0;
}
