#include<bits/stdc++.h>
#define int long long int

using namespace std;

void solve(){
int n,m;
cin>>n>>m;
  int s=0,e=0;
while(n--){
    int a,b;
    cin>>a>>b;

    if(a==m){
    s++;
    }
    if(b==m)e++;
}
if(s>0 && e>0){
    cout<<"YES"<<endl;
}
else{
    cout<<"NO"<<endl;
}
}

int32_t main() {
	// your code goes here
	int t;
	cin>>t;

	while(t--){
            solve();
	}
	return 0;
}
