#include<bits/stdc++.h>
using namespace std;
#define int long long int
#define beg begin()
#define en  end()

void solve(){

int n;
cin>>n;
vector<int> a(n);

for(int i=0;i<n;i++){cin>>a[i];}
sort(a.beg,a.en);
int f=0;
for(int i=0;i<n-1;i+=1){
    if((a[i+1]-a[i])<=1){

    }
    else{
            f=1;
        break;
    }
}

if(f==0){
    cout<<"YES"<<endl;
}
else{
        cout<<"NO"<<endl;
}
}







int32_t main(){
int t=1;
cin>>t;
while(t--){
    solve();
}




return 0;}
