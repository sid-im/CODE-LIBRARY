#include<bits/stdc++.h>
using namespace std;

void solve(){
int n,t;
cin>>n>>t;
string s;
cin>>s;
for(int i=0;i<t;i++){
    for(int k=0;k<n;k++){
        if(s[k]=='B' && s[k+1]=='G'){
            swap(s[k],s[k+1]);
            k++;
        }
    }
}
cout<<s<<endl;
}





int32_t main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */

    solve();



    return 0;
}
