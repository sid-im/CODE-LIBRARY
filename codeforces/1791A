#include<bits/stdc++.h>
#define int long long int
using namespace std;

void solve(){

    char c;
    cin>>c;
    bool f=0;
    string s="codeforces";
    for(int i=0;i<s.length();i++)

    {

        if(s[i]==c){
           f=1;
        }
    }
if(f==1){
    cout<<"YES"<<endl;
}
else{
       cout<<"NO"<<endl;
}
}




int32_t main(){

int t=1;
cin>>t;
while(t--){

    solve();
}
return 0;}
