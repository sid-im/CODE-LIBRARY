#include<iostream>
#include <bits/stdc++.h>
using namespace std;
void solve(){

    vector<string> arr(8);
    for(int i=0;i<8;i++) cin>>arr[i];

    for(int i=0;i<8;i++)
    {
        if(arr[i] == "RRRRRRRR")
        {
            cout<<"R"<<endl;
            return;
        }
    }

    cout<<"B"<<endl;;

}

int main(){
int t;
cin>>t;
while(t--){
    solve();
}

}
